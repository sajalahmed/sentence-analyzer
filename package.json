{
  "name": "wsd",
  "version": "1.0.0",
  "main": "app.js",
  "scripts": {
    "test": "NODE_ENV=development jest --detectOpenHandles",
    "start": "node app.js",
    "dev": "nodemon app.js",
    "model:generate": "npx sequelize --options-path ./src/db/.sequelize model:generate",
    "migrate": "npx sequelize --options-path ./src/db/.sequelize db:migrate",
    "migrate:undo": "npx sequelize --options-path ./src/db/.sequelize db:migrate:undo",
    "migrate:fresh": "npx sequelize --options-path ./src/db/.sequelize db:migrate:undo:all",
    "seed": "npx sequelize --options-path ./src/db/.sequelize db:seed:all",
    "seed:generate": "npx sequelize --options-path ./src/db/.sequelize seed:generate",
    "migrate:refresh": "npx sequelize --options-path ./src/db/.sequelize db:migrate:undo:all && npx sequelize --options-path ./src/db/config/.sequelize db:migrate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.3.1",
    "jest": "^29.7.0",
    "joi": "^17.13.3",
    "keycloak-connect": "^25.0.1",
    "node-fetch": "^2.7.0",
    "pg": "^8.12.0",
    "sequelize": "^6.37.3",
    "supertest": "^7.0.0",
    "winston": "^3.13.1",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "node-mocks-http": "^1.15.0",
    "nodemon": "^3.1.4",
    "sequelize-cli": "^6.6.2"
  }
}
